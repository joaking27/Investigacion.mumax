N := 128
setgridsize(N,N,1)

Lmin := 8.0
Lmax := 9.0
Lstep := 0.05

L := Lmin
setcellsize(L/N,L/N,L/N)

Msat = sqrt(2/mu0)
Aex = 1.0
Ku1 = 0.1
anisU = vector(1,0,0) 

m = vortex(1,-1)

tableadd(E_total)

tableAdd(E_anis)

tableAdd(E_custom)

tableAdd(E_demag)

tableAdd(E_exch)

tableAdd(E_mel)

tableAdd(E_zeeman)
tableaddvar(L,"L","")

for L=Lmin ; L<=Lmax; L+=Lstep {
    setcellsize(L/N,L/N,L/N)
    minimize()
    save(m)
    tablesave()
}
